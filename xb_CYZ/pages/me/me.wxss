.me .me-login{
  height: 200rpx;
  background-color: #434343;
}
.me .me-login image{
    width: 130rpx;
    height: 130rpx;
    display:inline-block;
    margin-left: 40rpx;
    background-color: #d2d2d3;
    border-radius: 50%;
    margin-top: 40rpx;
}
.me .me-login>text{
